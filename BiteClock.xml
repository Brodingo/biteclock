<!-- GuiXml>Controls>TopLevelControl>Dimensions/+Anchor/+Controls>Label>Anchor/ -->
<GuiXml>
    <Controls>
        <TopLevelControl name="BiteClock" movable="true" mouseEnabled="true" clampedToScreen="true">
            <Dimensions x="200" y="50"/>
            <Anchor point="TOPLEFT" relativeTo="GuiRoot" relativePoint="TOPLEFT" offsetX="0" offsetY="10"/>
            <Controls>
                <Label name="$(parent)Label" width="200" height="50" font="ZoFontEdit" inheritAlpha="true" wrapMode="TRUNCATE" color="FFFFFF" verticalAlignment="TOP" horizontalAlignment="LEFT">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)"  relativePoint="TOP" />
                </Label>
            </Controls>
        </TopLevelControl>
    </Controls>
</GuiXml>
